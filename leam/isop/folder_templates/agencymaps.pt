<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      lang="en"
      metal:use-macro="context/main_template/macros/master"
      i18n:domain="leam.isop">


<metal:slot fill-slot="style_slot">
  <link rel="stylesheet" class="required"
    href="http://openlayers.org/en/v3.4.0/css/ol.css" >
  <link rel="stylesheet" class="required"
    tal:define="navroot context/@@plone_portal_state/navigation_root_url"
    tal:attributes="href string:${navroot}/++resource++leam.isop/isop.css">
</metal:slot>

<metal:slot fill-slot="javascript_head_slot">
  <script src="http://openlayers.org/en/v3.4.0/build/ol.js" 
          type="text/javascript"></script>
</metal:slot>

<body>

  <!-- remove title and description -->
  <metal:slot fill-slot="content-title">
    <metal:content-title define-macro="content-title">
      </metal:content-title>
  </metal:slot>

  <metal:slot fill-slot="content-description">
    <metal:content-description define-macro="content-description">
      </metal:content-description>
  </metal:slot>


  <metal:slot fill-slot="content-core">
    <metal:content-core define-macro="content-core">

      <div id="map" class="map simmap"></div>
      <div id="controlp" class="controlp ol-control">
        <table class="agency-control">
        </table>
      </div>
  
      <script type="text/javascript" src="++resource++leam.isop/agencylist.js">
      </script>
  
    </metal:content-core>
  </metal:slot>
  
</body>
</html>

